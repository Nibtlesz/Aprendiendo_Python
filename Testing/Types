#TIPOS DE DATOS
print(type(4)) #INT
print(type(5.34)) #FLOAT
print(type("hola mundo")) #STRING
print(type(True)) #BOOL

#LA FUNCIÒN TYPE SIRVE PARA MOSTRAR QUE TIPO DE DATO ES

print(int(5.3))
print(float(6))
print(int(True))
print(bool(0))
print(str(50))

#LA FUNCIÒN INT, FLOAT, BOOL, STR SIRVEN PARA CONVERTIR UN TIPO DE DATO A OTRO. NO PUEDES CONVERTIR UN STRING A NUMERICO O BOOLEANO